command=$SCRIPT_DIR/$BLOCK_NAME
separator_block_width=20

[wifi]
command=$HOME/.config/i3/scripts/iface
interval=1
label=

[volume]
command=$HOME/.config/i3/scripts/volume
interval=1

[battery]
command=$HOME/.config/i3/scripts/battery
interval=1
label=

[load]
command=cut -d ' ' -f1 /proc/loadavg
interval=1
label=

[disk]
command=df -h "/" | tail -n 1 | awk '{print $4}'
interval=1
label=

[time]
command=date +' %H:%M   %a %d %b %Y '
interval=1
markup=pango
label=
